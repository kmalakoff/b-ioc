    "test": "tsds link && ts-swc tape test/**/*.test.cjs && tsds unlink",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "@types/tape": "^5.8.1",
    "node-version-use": "^2.1.5",
    "tape": "^5.9.0",
    "ts-dev-stack": "^1.21.3",
    "ts-swc-loaders": "^2.5.3",
    "tsds-config": "^0.2.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
